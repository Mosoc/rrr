{
  "name": "request-rectifier",
  "description": "Modification of HTTP Requests",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=8"
  },
  "main": "index.js",
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig-module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "mocha -r ts-node/register src/**/*.spec.ts",
    "clean": "trash build"
  },
  "keywords": [
    "request",
    "typescript"
  ],
  "author": "Bruno Anderson Chen <mosoc@colle.co>",
  "license": "ISC",
  "readmeFilename": "README.md",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.9",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "trash-cli": "^3.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "prettier": {
    "singleQuote": true
  }
}
